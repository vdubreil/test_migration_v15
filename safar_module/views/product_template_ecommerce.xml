<odoo>
    <record model="ir.ui.view" id="s_website_sale_product_template_form_view">
         <field name="name">Safar.website_sale.product_template_form_view</field>
         <field name="model">product.template</field>
         <field name="inherit_id" ref="website_sale.product_template_form_view"/>
         <field name="arch" type="xml">
             <xpath expr="//field[@name='website_id']" position="after">
                 <field name="s_lib_ecommerce"/>
             </xpath>
        </field>
    </record>
</odoo>